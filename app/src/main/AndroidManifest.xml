<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android = "http://schemas.android.com/apk/res/android"
          package = "com.example.contestschedule" >

    <uses-permission android:name = "android.permission.INTERNET" />

    <application
        android:allowBackup = "true"
        android:icon = "@mipmap/ic_launcher"
        android:label = "@string/app_name"
        android:roundIcon = "@mipmap/ic_launcher_round"
        android:supportsRtl = "true"
        android:theme = "@style/Theme.ContestSchedule" >
        <activity android:name = ".ui.developer_activity.DeveloperActivity" >
        </activity >
        <activity
            android:name = ".ui.setting_activity.SettingActivity"
            android:windowSoftInputMode = "adjustPan" >
        </activity >
        <activity android:name = ".ui.browser_activity.BrowserActivity" >
        </activity >
        <activity android:name = ".ui.main_activity.MainActivity" >
            <intent-filter >
                <action android:name = "android.intent.action.MAIN" />

                <category android:name = "android.intent.category.LAUNCHER" />
            </intent-filter >
        </activity >
    </application >

</manifest >